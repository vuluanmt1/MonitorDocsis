<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/colorWhite"
    android:layout_height="match_parent">
    <ProgressBar
        android:id="@+id/progBar"
        style="@android:style/Widget.Material.ProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:indeterminate="false"
        android:indeterminateTint="#2264cf"
        app:layout_constraintBottom_toBottomOf="@+id/scrollView"
        app:layout_constraintEnd_toEndOf="@+id/scrollView"
        app:layout_constraintStart_toStartOf="@+id/scrollView"
        app:layout_constraintTop_toTopOf="@+id/scrollView"
        tools:targetApi="lollipop" />
    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">
        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fillViewport="true">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginEnd="5dp"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/txt_search_his"
                        android:layout_width="200dp"
                        android:layout_height="35dp"
                        android:layout_weight="1"
                        android:contentDescription="@string/btn_gpon"
                        android:paddingRight="10dp"
                        android:scaleType="fitCenter"
                        android:visibility="visible"
                        app:srcCompat="@drawable/button_lichsu" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layout_search"
                    android:layout_width="470dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/custom_layout"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="5dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/txt_start_date"
                            android:layout_width="150dp"
                            android:layout_height="30dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginLeft="25dp"
                            android:gravity="center"
                            android:background="@drawable/custom_date_start"
                            android:hint="Ngày bắt đầu"
                            android:textColor="#141414"
                            android:textSize="15sp" />
                        <TextView
                            android:id="@+id/txt_end_date"
                            android:layout_width="150dp"
                            android:layout_height="30dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginLeft="20dp"
                            android:gravity="center"
                            android:background="@drawable/custom_date_start"
                            android:ems="10"
                            android:hint="Ngày kết thúc"
                            android:textColor="#000000"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="370dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:text="Cảnh Báo"
                            android:textColor="#000000"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <Switch
                            android:id="@+id/swt_warning"
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:text="" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="430dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:text="Fec"
                            android:textColor="#000000"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/edt_min_fec"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Min"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edt_max_fec"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:layout_marginLeft="5dp"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Max"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:text="UnFec"
                            android:textColor="#000000"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/edt_min_Unfec"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Min"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edt_max_Unfec"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:layout_marginLeft="5dp"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Max"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="430dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:text="SNR"
                            android:textColor="#000000"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/edt_min_Snr"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Min"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edt_max_Snr"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Max"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:text="Mer"
                            android:textColor="#000000"
                            android:textSize="15dp"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/edt_min_Mer"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Min"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edt_max_Mer"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Max"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="430dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:text="TxPw"
                            android:textColor="#000000"
                            android:textSize="15dp"
                            android:textStyle="bold" />
                        <EditText
                            android:id="@+id/edt_min_Tx"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Min"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />
                        <EditText
                            android:id="@+id/edt_max_Tx"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Max"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />
                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:text="RxPw"
                            android:textColor="#000000"
                            android:textSize="15dp"
                            android:textStyle="bold" />
                        <EditText
                            android:id="@+id/edt_min_Rx"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Min"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />
                        <EditText
                            android:id="@+id/edt_max_Rx"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="Max"
                            android:inputType="numberDecimal"
                            android:textColor="#000000"
                            android:textSize="14sp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="35dp"
                        android:layout_marginStart="130dp"
                        android:layout_marginTop="3dp"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/txt_search_ls"
                            android:layout_width="200dp"
                            android:layout_height="35dp"
                            android:layout_weight="1"
                            android:contentDescription="@string/btn_gpon"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:scaleType="fitCenter"
                            android:visibility="visible"
                            app:srcCompat="@drawable/button_timkiem" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="50dp"
                        android:layout_height="30dp"
                        android:text="Mac:"
                        android:textColor="#292424"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/txt_mac_his"
                        android:layout_width="120dp"
                        android:layout_height="30dp"
                        android:text="Mac"
                        android:textColor="#E61212"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                    <TextView
                        android:layout_width="50dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="3dp"
                        android:text="Địa chỉ:"
                        android:textColor="#292424"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/txt_address_his"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:text="Mac"
                        android:textColor="#E61212"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginTop="3dp"
                    android:gravity="center"
                    android:orientation="horizontal">
                    <HorizontalScrollView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fillViewport="true">
                        <ScrollView
                            android:id="@+id/scrollView1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="1.0">
                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent">
                                <TextView
                                    android:id="@+id/txt_time"
                                    android:layout_width="122dp"
                                    android:layout_height="match_parent"
                                    android:background="@color/colorDarkBlue"
                                    android:gravity="center"
                                    android:text=" Time "
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                                <TextView
                                    android:id="@+id/txt_status"
                                    android:layout_width="70dp"
                                    android:layout_height="40dp"
                                    android:layout_marginLeft="3dp"
                                    android:textColor="#FFFFFF"
                                    android:gravity="center"
                                    android:background="@color/colorDarkBlue"
                                    android:text="Trạng thái"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                                <TextView
                                    android:id="@+id/txt_snr"
                                    android:layout_width="50dp"
                                    android:layout_height="40dp"
                                    android:layout_marginLeft="3dp"
                                    android:textColor="#FFFFFF"
                                    android:gravity="center"
                                    android:background="@color/colorDarkBlue"
                                    android:text=" SNR "
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                                <TextView
                                    android:id="@+id/txt_mer"
                                    android:layout_width="50dp"
                                    android:layout_height="40dp"
                                    android:layout_marginLeft="3dp"
                                    android:background="@color/colorDarkBlue"
                                    android:textColor="#FFFFFF"
                                    android:gravity="center"
                                    android:text=" MER  "
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                                <TextView
                                    android:id="@+id/txt_fec"
                                    android:layout_width="50dp"
                                    android:layout_height="40dp"
                                    android:layout_marginLeft="3dp"
                                    android:background="@color/colorDarkBlue"
                                    android:textColor="#FFFFFF"
                                    android:gravity="center"
                                    android:text="Fec"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                                <TextView
                                    android:id="@+id/txt_unfec"
                                    android:layout_width="50dp"
                                    android:layout_height="40dp"
                                    android:layout_marginLeft="3dp"
                                    android:background="@color/colorDarkBlue"
                                    android:textColor="#FFFFFF"
                                    android:gravity="center"
                                    android:text="UnFec"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                                <TextView
                                    android:id="@+id/txt_txpw"
                                    android:layout_width="50dp"
                                    android:layout_height="40dp"
                                    android:layout_marginLeft="3dp"
                                    android:background="@color/colorDarkBlue"
                                    android:textColor="#FFFFFF"
                                    android:gravity="center"
                                    android:text="TxPw"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                                <TextView
                                    android:id="@+id/txt_rxpw"
                                    android:layout_width="50dp"
                                    android:layout_height="40dp"
                                    android:layout_marginLeft="3dp"
                                    android:background="@color/colorDarkBlue"
                                    android:textColor="#FFFFFF"
                                    android:gravity="center"
                                    android:text="RxPw"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>
                        </ScrollView>
                    </HorizontalScrollView>
                </LinearLayout>
                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fillViewport="true">
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recView"
                        android:layout_marginTop="3dp"
                        android:layout_marginBottom="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    </androidx.recyclerview.widget.RecyclerView>
                </HorizontalScrollView>
            </LinearLayout>
        </HorizontalScrollView>
    </ScrollView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="horizontal">
            <TextView
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginStart="0dp"
                android:background="@color/colorLightGray"
                android:gravity="center"
                android:text="Số lượng :"
                android:textColor="#000000"
                android:textSize="14dp"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/txt_total_data"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_marginStart="0dp"
                android:background="@color/colorLightGray"
                android:gravity="left"
                android:text=""
                android:textColor="#000000"
                android:textSize="14dp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>